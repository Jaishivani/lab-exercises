def password_strength_checker(password):
    if len(password) < 6:
        print("Weak: Password is less than 6 characters long")
    elif 6 <= len(password) <= 10:
        print("Medium: Password is between 6 and 10 characters long")
    else:
        print("Strong: Password is more than 10 characters long")

# Test the function with different passwords
password_strength_checker("12345")          # Weak
password_strength_checker("123456789")      # Medium
password_strength_checker("1234567890123")  # Strong
